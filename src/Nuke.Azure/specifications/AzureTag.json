  "name": "AzureTag",
  "officialUrl": "https://docs.microsoft.com/en-us/cli/azure/tag?view=azure-cli-latest",
  "help": "Manage resource tags.",
  "pathExecutable": "az",
  "tasks": [
    {
      "postfix": "AddValue",
      "definiteArgument": "tag add-value",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}",
            "help": "The name of the tag."
          },
          {
            "name": "Value",
            "type": "string",
            "format": "--value {value}",
            "help": "The value of the tag to create."
          }
        ]
      }
    },
    {
      "postfix": "Create",
      "definiteArgument": "tag create",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}",
            "help": "The name of the tag to create."
          }
        ]
      }
    },
    {
      "postfix": "Delete",
      "definiteArgument": "tag delete",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}",
            "help": "The name of the tag."
          }
        ]
      }
    },
    {
      "postfix": "List",
      "definiteArgument": "tag list",
      "settingsClass": {}
    },
    {
      "postfix": "RemoveValue",
      "definiteArgument": "tag remove-value",
      "settingsClass": {
        "properties": [
          {
            "name": "Name",
            "type": "string",
            "format": "--name {value}",
            "help": "The name of the tag."
          },
          {
            "name": "Value",
            "type": "string",
            "format": "--value {value}",
            "help": "The value of the tag to delete."
          }
        ]
      }
    }
  ],
  "commonTaskProperties": [
    {
      "name": "Debug",
      "type": "string",
      "format": "--debug {value}",
      "help": "Increase logging verbosity to show all debug logs."
    },
    {
      "name": "Help",
      "type": "string",
      "format": "--help {value}",
      "help": "Show this help message and exit."
    },
    {
      "name": "Output",
      "type": "AzureOutput",
      "format": "--output {value}",
      "help": "Output format."
    },
    {
      "name": "Query",
      "type": "string",
      "format": "--query {value}",
